{
  "name": "advanced_frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite",
    "start:dev": "concurrently \"yarn start\" \"yarn start:dev:server\"",
    "start:dev:vite": "concurrently \"yarn start:vite\" \"yarn start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production apiUrl=https://advanced-frontend.vercel.app",
    "build:dev": "webpack --env mode=development",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint --fix \"**/*.scss\"",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "yarn test:ui:json && yarn test:ui:html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "postinstall": "rm -rf ./node_modules/.cache",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
