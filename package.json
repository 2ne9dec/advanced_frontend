    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-i18next-extract": "^0.9.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
